@prefix ds: <https://raw.githubusercontent.com/nsai-uio/ExeKGOntology/main/ds_exeKGOntology.ttl#> .
@prefix ml: <https://raw.githubusercontent.com/nsai-uio/ExeKGOntology/main/ml_exeKGOntology.ttl#> .
@prefix visu: <https://raw.githubusercontent.com/nsai-uio/ExeKGOntology/main/visu_exeKGOntology.ttl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ds:MLPipeline a ds:Pipeline ;
    ds:hasInputDataPath "C:\\Users\\klr2rng\\Documents\\projects\\ExeKGLib-internal\\examples\\data\\dummy_data.csv"^^xsd:string ;
    ds:hasPlotsOutputDir "C:\\Users\\klr2rng\\Documents\\projects\\ExeKGLib-internal\\examples\\plots\\MLPipeline"^^xsd:string ;
    ds:hasStartTask ml:Concatenation1 .

ds:feature_1 a ds:DataEntity,
        ds:Numerical,
        ds:Vector ;
    ds:hasSource "feature_1"^^xsd:string .

ds:feature_2 a ds:DataEntity,
        ds:Numerical,
        ds:Vector ;
    ds:hasSource "feature_2"^^xsd:string .

ds:feature_3 a ds:DataEntity,
        ds:Numerical,
        ds:Vector ;
    ds:hasSource "feature_3"^^xsd:string .

ds:feature_4 a ds:DataEntity,
        ds:Numerical,
        ds:Vector ;
    ds:hasSource "feature_4"^^xsd:string .

ds:feature_5 a ds:DataEntity,
        ds:Numerical,
        ds:Vector ;
    ds:hasSource "feature_5"^^xsd:string .

ds:label a ds:Categorical,
        ds:DataEntity,
        ds:Vector ;
    ds:hasSource "label"^^xsd:string .

ml:AccuracyScoreMethod1 a ml:AccuracyScoreMethod .

ml:BinaryClassification1 a ml:BinaryClassification ;
    ds:hasNextTask ml:Test1 ;
    ml:hasBinaryClassificationMethod ml:SVCMethod1 ;
    ml:hasTrainInput ml:DataInTrainX_BinaryClassification1_1,
        ml:DataInTrainY_BinaryClassification1_1 ;
    ml:hasTrainOutput ml:DataOutTrainModelSVCMethod .

ml:Concatenation1 a ml:Concatenation ;
    ds:hasNextTask ml:DataSplitting1 ;
    ml:hasConcatenationInput ml:DataInConcatenation_Concatenation1_1,
        ml:DataInConcatenation_Concatenation1_2,
        ml:DataInConcatenation_Concatenation1_3,
        ml:DataInConcatenation_Concatenation1_4,
        ml:DataInConcatenation_Concatenation1_5 ;
    ml:hasConcatenationMethod ml:ConcatenationMethod1 ;
    ml:hasConcatenationOutput ml:DataOutConcatenatedDataConcatenationMethod .

ml:ConcatenationMethod1 a ml:ConcatenationMethod .

ml:DataInConcatenation_Concatenation1_1 a ml:DataInConcatenation ;
    ds:hasReference ds:feature_1 .

ml:DataInConcatenation_Concatenation1_2 a ml:DataInConcatenation ;
    ds:hasReference ds:feature_2 .

ml:DataInConcatenation_Concatenation1_3 a ml:DataInConcatenation ;
    ds:hasReference ds:feature_3 .

ml:DataInConcatenation_Concatenation1_4 a ml:DataInConcatenation ;
    ds:hasReference ds:feature_4 .

ml:DataInConcatenation_Concatenation1_5 a ml:DataInConcatenation ;
    ds:hasReference ds:feature_5 .

ml:DataInDataSplittingX_DataSplitting1_1 a ml:DataInDataSplittingX ;
    ds:hasReference ml:DataOutConcatenatedDataConcatenationMethod .

ml:DataInDataSplittingY_DataSplitting1_1 a ml:DataInDataSplittingY ;
    ds:hasReference ds:label .

ml:DataInPredictedY_PerformanceCalculation1_1 a ml:DataInPredictedY ;
    ds:hasReference ml:DataOutPredictedValueTestTestMethod .

ml:DataInPredictedY_PerformanceCalculation2_1 a ml:DataInPredictedY ;
    ds:hasReference ml:DataOutPredictedValueTestTestMethod .

ml:DataInPredictedY_PerformanceCalculation3_1 a ml:DataInPredictedY ;
    ds:hasReference ml:DataOutPredictedValueTestTestMethod .

ml:DataInPredictedY_PerformanceCalculation4_1 a ml:DataInPredictedY ;
    ds:hasReference ml:DataOutPredictedValueTestTestMethod .

ml:DataInRealY_PerformanceCalculation1_1 a ml:DataInRealY ;
    ds:hasReference ml:DataOutSplittedTestDataYTrainTestSplitMethod .

ml:DataInRealY_PerformanceCalculation2_1 a ml:DataInRealY ;
    ds:hasReference ml:DataOutSplittedTestDataYTrainTestSplitMethod .

ml:DataInRealY_PerformanceCalculation3_1 a ml:DataInRealY ;
    ds:hasReference ml:DataOutSplittedTestDataYTrainTestSplitMethod .

ml:DataInRealY_PerformanceCalculation4_1 a ml:DataInRealY ;
    ds:hasReference ml:DataOutSplittedTestDataYTrainTestSplitMethod .

ml:DataInTestModel_Test1_1 a ml:DataInTestModel ;
    ds:hasReference ml:DataOutTrainModelSVCMethod .

ml:DataInTestX_Test1_1 a ml:DataInTestX ;
    ds:hasReference ml:DataOutSplittedTestDataXTrainTestSplitMethod .

ml:DataInTrainX_BinaryClassification1_1 a ml:DataInTrainX ;
    ds:hasReference ml:DataOutSplittedTrainDataXTrainTestSplitMethod .

ml:DataInTrainY_BinaryClassification1_1 a ml:DataInTrainY ;
    ds:hasReference ml:DataOutSplittedTrainDataYTrainTestSplitMethod .

ml:DataSplitting1 a ml:DataSplitting ;
    ds:hasNextTask ml:BinaryClassification1 ;
    ml:hasDataSplittingInput ml:DataInDataSplittingX_DataSplitting1_1,
        ml:DataInDataSplittingY_DataSplitting1_1 ;
    ml:hasDataSplittingMethod ml:TrainTestSplitMethod1 ;
    ml:hasDataSplittingOutput ml:DataOutSplittedTestDataXTrainTestSplitMethod,
        ml:DataOutSplittedTestDataYTrainTestSplitMethod,
        ml:DataOutSplittedTrainDataXTrainTestSplitMethod,
        ml:DataOutSplittedTrainDataYTrainTestSplitMethod .

ml:F1ScoreMethod1 a ml:F1ScoreMethod .

ml:PerformanceCalculation1 a ml:PerformanceCalculation ;
    ds:hasNextTask ml:PerformanceCalculation2 ;
    ml:hasPerformanceCalculationInput ml:DataInPredictedY_PerformanceCalculation1_1,
        ml:DataInRealY_PerformanceCalculation1_1 ;
    ml:hasPerformanceCalculationMethod ml:AccuracyScoreMethod1 ;
    ml:hasPerformanceCalculationOutput ml:DataOutScoreAccuracyScoreMethod .

ml:PerformanceCalculation2 a ml:PerformanceCalculation ;
    ds:hasNextTask ml:PerformanceCalculation3 ;
    ml:hasPerformanceCalculationInput ml:DataInPredictedY_PerformanceCalculation2_1,
        ml:DataInRealY_PerformanceCalculation2_1 ;
    ml:hasPerformanceCalculationMethod ml:F1ScoreMethod1 ;
    ml:hasPerformanceCalculationOutput ml:DataOutScoreF1ScoreMethod .

ml:PerformanceCalculation3 a ml:PerformanceCalculation ;
    ds:hasNextTask ml:PerformanceCalculation4 ;
    ml:hasPerformanceCalculationInput ml:DataInPredictedY_PerformanceCalculation3_1,
        ml:DataInRealY_PerformanceCalculation3_1 ;
    ml:hasPerformanceCalculationMethod ml:PrecisionScoreMethod1 ;
    ml:hasPerformanceCalculationOutput ml:DataOutScorePrecisionScoreMethod .

ml:PerformanceCalculation4 a ml:PerformanceCalculation ;
    ds:hasNextTask visu:CanvasCreation1 ;
    ml:hasPerformanceCalculationInput ml:DataInPredictedY_PerformanceCalculation4_1,
        ml:DataInRealY_PerformanceCalculation4_1 ;
    ml:hasPerformanceCalculationMethod ml:RecallScoreMethod1 ;
    ml:hasPerformanceCalculationOutput ml:DataOutScoreRecallScoreMethod .

ml:PrecisionScoreMethod1 a ml:PrecisionScoreMethod .

ml:RecallScoreMethod1 a ml:RecallScoreMethod .

ml:SVCMethod1 a ml:SVCMethod ;
    ml:hasParamRandomState 0 .

ml:Test1 a ml:Test ;
    ds:hasNextTask ml:PerformanceCalculation1 ;
    ml:hasTestInput ml:DataInTestModel_Test1_1,
        ml:DataInTestX_Test1_1 ;
    ml:hasTestMethod ml:TestMethod1 ;
    ml:hasTestOutput ml:DataOutPredictedValueTestTestMethod .

ml:TestMethod1 a ml:TestMethod .

ml:TrainTestSplitMethod1 a ml:TrainTestSplitMethod ;
    ml:hasParamRandomState 0 ;
    ml:hasParamTestSize "0.2"^^xsd:float .

visu:BarMethod1 a visu:BarMethod ;
    visu:hasParamTitle "Test Accuracy & F1"^^xsd:string .

visu:BarMethod2 a visu:BarMethod ;
    visu:hasParamTitle "Test Precision & Recall"^^xsd:string .

visu:BarPlotting1 a visu:BarPlotting ;
    ds:hasNextTask visu:BarPlotting2 ;
    visu:hasBarPlottingMethod visu:BarMethod1 ;
    visu:hasPlottingInput visu:DataInToPlot_BarPlotting1_1,
        visu:DataInToPlot_BarPlotting1_2 .

visu:BarPlotting2 a visu:BarPlotting ;
    visu:hasBarPlottingMethod visu:BarMethod2 ;
    visu:hasPlottingInput visu:DataInToPlot_BarPlotting2_1,
        visu:DataInToPlot_BarPlotting2_2 .

visu:CanvasCreation1 a visu:CanvasCreation ;
    ds:hasNextTask visu:BarPlotting1 ;
    visu:hasCanvasCreationMethod visu:CanvasMethod1 .

visu:CanvasMethod1 a visu:CanvasMethod ;
    visu:hasParamFigureSize "10 10"^^xsd:string ;
    visu:hasParamLayout "2 1"^^xsd:string .

visu:DataInToPlot_BarPlotting1_1 a visu:DataInToPlot ;
    ds:hasReference ml:DataOutScoreAccuracyScoreMethod .

visu:DataInToPlot_BarPlotting1_2 a visu:DataInToPlot ;
    ds:hasReference ml:DataOutScoreF1ScoreMethod .

visu:DataInToPlot_BarPlotting2_1 a visu:DataInToPlot ;
    ds:hasReference ml:DataOutScorePrecisionScoreMethod .

visu:DataInToPlot_BarPlotting2_2 a visu:DataInToPlot ;
    ds:hasReference ml:DataOutScoreRecallScoreMethod .

ml:DataOutConcatenatedDataConcatenationMethod a ds:Tabular,
        ml:DataOutConcatenatedData .

ml:DataOutScoreAccuracyScoreMethod a ds:SingleValue,
        ml:DataOutScore .

ml:DataOutScoreF1ScoreMethod a ds:SingleValue,
        ml:DataOutScore .

ml:DataOutScorePrecisionScoreMethod a ds:SingleValue,
        ml:DataOutScore .

ml:DataOutScoreRecallScoreMethod a ds:SingleValue,
        ml:DataOutScore .

ml:DataOutSplittedTestDataXTrainTestSplitMethod a ds:Matrix,
        ds:Tabular,
        ds:Vector,
        ml:DataOutSplittedTestDataX .

ml:DataOutSplittedTrainDataXTrainTestSplitMethod a ds:Matrix,
        ds:Tabular,
        ds:Vector,
        ml:DataOutSplittedTrainDataX .

ml:DataOutSplittedTrainDataYTrainTestSplitMethod a ds:Matrix,
        ds:Tabular,
        ds:Vector,
        ml:DataOutSplittedTrainDataY .

ml:DataOutTrainModelSVCMethod a ds:Model,
        ml:DataOutTrainModel .

ml:DataOutPredictedValueTestTestMethod a ds:Matrix,
        ds:Vector,
        ml:DataOutPredictedValueTest .

ml:DataOutSplittedTestDataYTrainTestSplitMethod a ds:Matrix,
        ds:Tabular,
        ds:Vector,
        ml:DataOutSplittedTestDataY .
