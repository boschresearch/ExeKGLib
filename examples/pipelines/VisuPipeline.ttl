@prefix ds: <https://raw.githubusercontent.com/nsai-uio/ExeKGOntology/main/ds_exeKGOntology.ttl#> .
@prefix visu: <https://raw.githubusercontent.com/nsai-uio/ExeKGOntology/main/visu_exeKGOntology.ttl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ds:VisuPipeline a ds:Pipeline ;
    ds:hasInputDataPath "C:\\Users\\klr2rng\\Documents\\projects\\ExeKGLib-internal\\examples\\data\\dummy_data.csv"^^xsd:string ;
    ds:hasPlotsOutputDir "C:\\Users\\klr2rng\\Documents\\projects\\ExeKGLib-internal\\examples\\plots\\VisuPipeline"^^xsd:string ;
    ds:hasStartTask visu:CanvasCreation1 .

ds:feature_1 a ds:DataEntity,
        ds:TimeSeries,
        ds:Vector ;
    ds:hasSource "feature_1"^^xsd:string .

ds:feature_2 a ds:DataEntity,
        ds:TimeSeries,
        ds:Vector ;
    ds:hasSource "feature_2"^^xsd:string .

visu:CanvasCreation1 a visu:CanvasCreation ;
    ds:hasNextTask visu:LinePlot1 ;
    visu:hasCanvasCreationMethod visu:CanvasMethod1 .

visu:CanvasMethod1 a visu:CanvasMethod ;
    visu:hasParamFigureSize "10 5"^^xsd:string ;
    visu:hasParamLayout "1 2"^^xsd:string .

visu:DataInToPlot1_1 a visu:DataInToPlot ;
    ds:hasReference ds:feature_1 .

visu:DataInToPlot2_1 a visu:DataInToPlot ;
    ds:hasReference ds:feature_2 .

visu:LinePlot1 a visu:LinePlot ;
    ds:hasInput visu:DataInToPlot1_1 ;
    ds:hasNextTask visu:LinePlot2 ;
    visu:hasLinePlotMethod visu:Plot1 .

visu:LinePlot2 a visu:LinePlot ;
    ds:hasInput visu:DataInToPlot2_1 ;
    visu:hasLinePlotMethod visu:Plot2 .

visu:Plot1 a visu:Plot ;
    visu:hasParamTitle "Feature 1"^^xsd:string .

visu:Plot2 a visu:Plot ;
    visu:hasParamTitle "Feature 2"^^xsd:string .
