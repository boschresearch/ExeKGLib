@prefix ds: <https://raw.githubusercontent.com/nsai-uio/ExeKGOntology/main/ds_exeKGOntology.ttl#> .
@prefix stats: <https://raw.githubusercontent.com/nsai-uio/ExeKGOntology/main/stats_exeKGOntology.ttl#> .
@prefix visu: <https://raw.githubusercontent.com/nsai-uio/ExeKGOntology/main/visu_exeKGOntology.ttl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ds:StatsPipeline a ds:Pipeline ;
    ds:hasInputDataPath "C:\\Users\\klr2rng\\Documents\\projects\\ExeKGLib-internal\\examples\\data\\dummy_data.csv"^^xsd:string ;
    ds:hasPlotsOutputDir "C:\\Users\\klr2rng\\Documents\\projects\\ExeKGLib-internal\\examples\\plots\\StatsPipeline"^^xsd:string ;
    ds:hasStartTask stats:CentralTendencyMeasure1 .

stats:CentralTendencyMeasure1 a stats:CentralTendencyMeasure ;
    ds:hasNextTask stats:DispersionMeasure1 ;
    stats:hasCentralTendencyMeasureMethod stats:MeanMethod1 ;
    stats:hasStatisticCalculationInput stats:DataInStatisticCalculation_CentralTendencyMeasure1_1 ;
    stats:hasStatisticCalculationOutput stats:DataOutStatisticCalculationMeanMethod .

stats:DataInStatisticCalculation_CentralTendencyMeasure1_1 a stats:DataInStatisticCalculation ;
    ds:hasReference ds:feature_1 .

stats:DataInStatisticCalculation_DispersionMeasure1_1 a stats:DataInStatisticCalculation ;
    ds:hasReference ds:feature_1 .

stats:DispersionMeasure1 a stats:DispersionMeasure ;
    ds:hasNextTask visu:CanvasCreation1 ;
    stats:hasDispersionMeasureMethod stats:StdMethod1 ;
    stats:hasStatisticCalculationInput stats:DataInStatisticCalculation_DispersionMeasure1_1 ;
    stats:hasStatisticCalculationOutput stats:DataOutStatisticCalculationStdMethod .

stats:MeanMethod1 a stats:MeanMethod .

stats:StdMethod1 a stats:StdMethod .

visu:BarMethod1 a visu:BarMethod ;
    visu:hasParamTitle "Feature 1's Mean and Standard Deviation"^^xsd:string .

visu:BarPlotting1 a visu:BarPlotting ;
    visu:hasBarPlottingMethod visu:BarMethod1 ;
    visu:hasPlottingInput visu:DataInToPlot_BarPlotting1_1,
        visu:DataInToPlot_BarPlotting1_2 .

visu:CanvasCreation1 a visu:CanvasCreation ;
    ds:hasNextTask visu:LinePlotting1 ;
    visu:hasCanvasCreationMethod visu:CanvasMethod1 .

visu:CanvasMethod1 a visu:CanvasMethod ;
    visu:hasParamFigureSize "10 10"^^xsd:string ;
    visu:hasParamLayout "2 1"^^xsd:string .

visu:DataInToPlot_BarPlotting1_1 a visu:DataInToPlot ;
    ds:hasReference stats:DataOutStatisticCalculationMeanMethod .

visu:DataInToPlot_BarPlotting1_2 a visu:DataInToPlot ;
    ds:hasReference stats:DataOutStatisticCalculationStdMethod .

visu:DataInToPlot_LinePlotting1_1 a visu:DataInToPlot ;
    ds:hasReference ds:feature_1 .

visu:LinePlotting1 a visu:LinePlotting ;
    ds:hasNextTask visu:BarPlotting1 ;
    visu:hasLinePlottingMethod visu:PlotMethod1 ;
    visu:hasPlottingInput visu:DataInToPlot_LinePlotting1_1 .

visu:PlotMethod1 a visu:PlotMethod ;
    visu:hasParamTitle "Feature 1"^^xsd:string .

stats:DataOutStatisticCalculationMeanMethod a ds:Matrix,
        ds:SingleValue,
        ds:Tabular,
        ds:Tensor,
        ds:Vector,
        stats:DataOutStatisticCalculation .

stats:DataOutStatisticCalculationStdMethod a ds:Matrix,
        ds:SingleValue,
        ds:Tabular,
        ds:Tensor,
        ds:Vector,
        stats:DataOutStatisticCalculation .

ds:feature_1 a ds:DataEntity,
        ds:Numerical,
        ds:Vector ;
    ds:hasSource "feature_1"^^xsd:string .
